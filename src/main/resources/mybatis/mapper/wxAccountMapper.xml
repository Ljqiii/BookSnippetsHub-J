<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ljqiii.testspringsecurity.mapper.UserMapper">

    <resultMap id="user" type="com.ljqiii.model.User"/>
    <parameterMap id="user" type="com.ljqiii.model.User"/>

    <select id="findByUser" parameterType="java.lang.String" resultMap="user">
        select * from user where name=#{1}
    </select>

    <select id="getUserList" resultMap="user">
        SELECT  * FROM user
    </select>


</mapper>